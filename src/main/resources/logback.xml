<?xml version="1.0"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%date{yyyy-MM-dd HH:mm:ss.SSS} %-5level %class.%method\(\)#%L: %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="no.ezand.boilerplate.docker" level="TRACE"/>

    <root level="DEBUG">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
